@if(Model == 1)
{
    <span style="color: #fff; background: #199DD9">Đang xử lý</span>
}
else if (Model == 2)
{
    <span style="color: #fff; background: #EF9139">Đang giao hàng</span>
}
else if (Model == 3)
{
    <span style="color: #fff; background: #28a745">Đã hoàn tất</span>
}
else if (Model == 4)
{
    <span style="color: #fff; background: #fe0000">Hủy đơn</span>
}